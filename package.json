    "main": "js/app.js",
    "scripts": {
        "build": "tsc",
        "start": "npm run build && electron .",
        "package-mac": "electron-packager --overwrite --darwinDarkModeSupport=true --icon icons/TMXValidator.icns --app-bundle-id=com.maxprograms.tmxvalidator --ignore 'ts' --ignore 'src' --ignore 'jars' .",
